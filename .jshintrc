{
    "browser": true,
    "nonstandard": true,
    "globals": {
        "d3": true,
        "moment": true,
        "React": true
    },
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 4,
